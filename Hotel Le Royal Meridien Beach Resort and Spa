void idle()
{
    if(!mouseDown)
    {
        xrot += 0.3f;
        yrot += 0.4f;
    }
    glutPostRedisplay();
}

void mouseMotion(int x,int y)
{
    if(mouseDown)
    {
        yrot = x - xdiff;
        xrot = y + ydiff;
        glutPostRedisplay();
    }
}

void mouse(int button, int state, int x, int y)
{
    if(button == GLUT_LEFT_BUTTON && state == GLUT_DOWN)
    {
        mouseDown = true;
        xdiff = x - yrot;
        ydiff = -y + xrot;
    }
    else
        mouseDown = false;
}


void keyboard(unsigned char key, int x, int y){

    //Untuk zoom in
    if (key=='q')d +=10;
    else if(key=='Q')d +=10;
    //Untuk zoom out
    if (key=='e')d +=-10;
    else if(key=='E')d +=-10;

    switch (key)
    {
    case '1': //fungsi keyboard untuk melihat interior
        if (is_depth)
        {
            is_depth = 0;
            glDisable(GL_DEPTH_TEST);
        }
        else{
        is_depth = 1;
        glEnable(GL_DEPTH_TEST);
        }

    // ATAS - BAWAH
    case 'w':
    case 'W':
		angleY += -5.0;
		break;
	case 's':
	case 'S':
        angleY += 5.0;
		break;

    // KANAN - KIRI
    case 'd':
    case 'D':
		angleX += -5.0;
		break;
	case 'a':
    case 'A':
		angleX += 5.0;
		break;

}
tampil();
}

void ukuran(int lebar, int tinggi)
{
	    if (tinggi == 0) tinggi = 1;

    glMatrixMode(GL_PROJECTION);
    glLoadIdentity();
    gluPerspective(110.0, lebar / tinggi, 5.0, 1000.0);
    glTranslatef(0.0, -200.0, -600.0);
    glMatrixMode(GL_MODELVIEW);
}
